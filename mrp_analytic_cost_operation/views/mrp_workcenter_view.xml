<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="mrp_workcenter_form_view_custom" model="ir.ui.view">
        <field name="name">mrp.workcenter.form.custom</field>
        <field name="model">mrp.workcenter</field>
        <field name="inherit_id" ref="mrp.mrp_workcenter_view" />
        <field name="arch" type="xml">
            <page name="general_info" position="after">
                <page name="analytic_cost_structure" string="Analytic Cost Structure">
                    <field name="cost_type_ids">
                        <tree editable="bottom">
                            <field name="work_center_id" />
                            <field name="product_id" />
                            <field name="factor" />
                        </tree>
                    </field>
                </page>
            </page>
        </field>
    </record>
</odoo>
